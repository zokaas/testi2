#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules/env-cmd/bin/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules/env-cmd/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules/env-cmd/bin/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules/env-cmd/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/env-cmd@10.1.0/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../env-cmd/bin/env-cmd.js" "$@"
else
  exec node  "$basedir/../env-cmd/bin/env-cmd.js" "$@"
fi
