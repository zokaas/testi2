#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules/nx/bin/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules/nx/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules/nx/bin/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules/nx/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/nx@20.0.0_@swc-node+register@1.10.9_@swc+core@1.7.26/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../nx/bin/nx-cloud.js" "$@"
fi
